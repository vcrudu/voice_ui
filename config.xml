        <icon height="50" src="res/icon/ios/Icon-Small-50.png" width="50" />
        <icon height="100" src="res/icon/ios/icon-Small-50@2x.png" width="100" />
        <icon height="167" src="res/icon/ios/Icon-83.5@2x.png" width="167" />
