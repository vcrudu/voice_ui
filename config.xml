    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-audioinput" spec="^0.3.0" />
    <plugin name="cordova-plugin-media" spec="^5.0.2" />
    <plugin name="cordova-plugin-ios-microphone-permissions" spec="^1.0.0" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-ble-central" spec="^1.1.4">
        <variable name="BLUETOOTH_USAGE_DESCRIPTION" value="Connect with medical devices." />
    </plugin>
    <plugin name="cordova-plugin-keyboard" spec="^1.2.0" />
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeyboardShrinksView" value="false" />
    <plugin name="cordova.plugin.nativealert" spec="https://github.com/vigneshuvi/cordova-plugin-nativealert.git" />
